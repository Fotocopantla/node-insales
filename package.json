{
  "name": "insales",
  "homepage": "https://github.com/pomeo/node-insales",
  "version": "1.0.3",
  "description": "Insales.ru module for node.js",
  "keywords": [
    "insales",
    "insales.ru",
    "api"
  ],
  "author": "Sergey Ovechkin <me@pomeo.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/pomeo/node-insales.git"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "js2xml": "^1.0.4",
    "restler": "^3.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.4",
    "esdoc": "^0.4.6",
    "istanbul": "^0.3.22",
    "mocha": "^2.3.3",
    "should": "^8.2.2",
    "xo": "^0.9.0"
  },
  "scripts": {
    "docs": "./node_modules/.bin/esdoc -c esdoc.json",
    "test": "xo && make test-once",
    "build": "precompile",
    "postinstall": "pick-precompiled"
  },
  "config": {
    "pre-compiled": {
      "dir": "dist",
      "files": [
        "lib/index.js"
      ]
    }
  },
  "bugs": {
    "url": "https://github.com/pomeo/node-insales/issues"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "space-before-function-paren": 0,
      "curly": 0
    },
    "envs": [
      "node",
      "mocha"
    ]
  }
}
